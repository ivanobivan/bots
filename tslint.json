{
  "extends": [
    "prettier",
    "tslint-config-prettier",
    "tslint-react",
    "tslint-react-recommended"
  ],
  "rulesDirectory": "tslint-plugin-prettier",
  "plugins": [
    "prettier"
  ],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "jsx-key": true,
    "jsx-no-bind": true,
    "no-unused-expression": true,
    "no-non-null-assertion": false,
    "no-shadowed-variable": false,
    "no-object-literal-type-assertion": false,
    "no-console": [
      true,
      "log"
    ],
    "no-implicit-dependencies": false,
    "no-submodule-imports": [
      false,
      "@src",
      "rxjs"
    ],
    "ter-indent": [
      4,
      "spaces",
      {
        "SwitchCase": false
      }
    ],
    "quotes": [
      "error",
      "double"
    ],
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 4,
        "strict-type-predicates": true
      }
    ],
    "prettier": [
      true,
      {
        "printWidth": 150,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "trailingComma": "none",
        "bracketSpacing": false
      }
    ],
    "arrow-parens": false,
    "arrow-return-shorthand": [
      false
    ],
    "comment-format": [
      true,
      "check-space"
    ],
    "import-blacklist": [
      true,
      "rxjs"
    ],
    "interface-over-type-literal": false,
    "interface-name": false,
    "array-type": [
      true,
      "generic"
    ],
    "max-line-length": [
      true,
      {
        "limit": 150,
        "ignore-pattern": "^import {(.*?)}"
      }
    ],
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "newline-before-return": false,
    "no-any": false,
    "max-classes-per-file": false,
    "no-empty": false,
    "no-empty-interface": false,
    "no-import-side-effect": [
      true
    ],
    "no-inferrable-types": [
      true,
      "ignore-params",
      "ignore-properties"
    ],
    "no-invalid-this": [
      true,
      "check-function-in-method"
    ],
    "no-null-keyword": false,
    "no-require-imports": false,
    "no-this-assignment": [
      true,
      {
        "allow-destructuring": true
      }
    ],
    "no-trailing-whitespace": true,
    "no-unused-variable": [
      false
    ],
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "one-variable-per-declaration": [
      false
    ],
    "only-arrow-functions": [
      true,
      "allow-declarations"
    ],
    "ordered-imports": [
      false
    ],
    "prefer-method-signature": false,
    "prefer-template": [
      true,
      "allow-single-concat"
    ],
    "quotemark": [
      true,
      "double",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "trailing-comma": [
      false,
      {
        "singleline": "never",
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "never"
        }
      }
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "type-literal-delimiter": true,
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "jsx-no-lambda": false
  }
}